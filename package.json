{
  "name": "ovirt-ui-components",
  "version": "0.1.2",
  "description": "Reusable components for Open Virtualization UIs",
  "main": "dist/ovirt-ui-components.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "cd showcase && webpack-dev-server --inline --open",
